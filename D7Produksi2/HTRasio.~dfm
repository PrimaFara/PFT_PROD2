  object QBarang: TOracleDataSet
    SQL.Strings = (
      'select distinct b.*, c.no_benang as pakan, c.nama_konstruksi'
      'from ipisma_db4.konversi_beam b'
      'left join ('
      
        '     select distinct a.*, b.nama_konstruksi from ipisma_db4.KODE' +
        '_PROD_KONS_D a'
      
        '     left join (select distinct kp, (lusi||'#39' x '#39'||pakan) as nama' +
        '_konstruksi from ipisma_db4.kode_prod_kons'
      '     ) b on a.kp=b.kp'
      'order by a.kp'
      ') c on b.kp=c.kp'
      'where b.aktif = '#39'1'#39
      'order by b.kp, c.no_benang, b.mesin asc'
      ''
      ''
      ''
      ''
      ''
      ''
      ''
      
        '/*select distinct b.*, c.lusi||'#39' x '#39'||c.pakan as pakan from ipis' +
        'ma_db4.konversi_beam b'
      'left join ipisma_db4.kode_prod_kons c on b.kp=c.kp'
      'where b.aktif = '#39'1'#39
      'order by b.kp asc*/'
      ''
      ''
      '')
    QBEDefinition.QBEFieldDefs = {
      040000000F000000040000004B4F4445010000000000050000004D4553494E01
      0000000000020000004B5001000000000005000000524153494F010000000000
      05000000414B54494601000000000004000000454449540100000000000A0000
      0054474C5F494E534552540100000000000800000054474C5F45444954010000
