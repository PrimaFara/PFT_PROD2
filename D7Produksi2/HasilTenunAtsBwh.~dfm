  object QBrowseTotal10: TOracleDataSet
    SQL.Strings = (
      'SELECT * FROM (select '
      
        'sum(QTY_A) as prod_A,sum(QTY_B) as prod_B,sum(QTY_C) as prod_C,s' +
        'um(QTY_D) as prod_D,'
      'sum(QTY_E) as prod_E,sum(QTY_F) as prod_F,sum(QTY_G) as prod_G,'
      
        'sum(BK_A)+sum(BK_B)+sum(BK_C)+sum(BK_D)+sum(BK_E)+sum(BK_F)+sum(' +
        'BK_G)+'
      
        'sum(BS_A)+sum(BS_B)+sum(BS_C)+sum(BS_D)+sum(BS_E)+sum(BS_F)+sum(' +
        'BS_G) as total'
      'from (select * from'
      
        '(select * from ipisma_db4.V_LAP_DEF_TN_PROSES_GRUP2 where lokasi' +
        ' like:loom) '
      ':myparam)'
      ')'
      '')
    Variables.Data = {
      0300000002000000080000003A4D59504152414D010000000000000000000000
      050000003A4C4F4F4D050000000000000000000000}
