  object QProd_Mesin_Warping: TOracleDataSet
    SQL.Strings = (
      'select nama_mesin, konstruksi, qty,'
      'sum(pot) as pot, sum(kodi) as kodi, sum(kg) as kg'
      'from( select t.nama_mesin, t.konstruksi, count(*)as qty,'
      'sum(t.jml_potong) as pot, '
      'sum(t.jml_kodi) as kodi,'
      'sum(t.qty_kg) as kg'
      
        'from IPISMA_DB4.vdesain_beam_non_kanji t where t.tanggal>=trunc(' +
        ':pawal) and t.tanggal<=trunc(:pakhir)+1-1/86400 and t.nama_mesin' +
        ' not like '#39'%MITRA%'#39
      'and t.keterangan is null'
      'group by t.nama_mesin, t.konstruksi)'
      'group by nama_mesin, konstruksi, QTY'
      'order by nama_mesin')
    Variables.Data = {
      0300000002000000060000003A504157414C0C00000007000000787407010101
      0100000000070000003A50414B4849520C000000070000007874081001010100
