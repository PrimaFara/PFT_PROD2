  object QDimPakan: TOracleDataSet
    SQL.Strings = (
      
        'select sum(nvl(hari,0)) as helai, sum(nvl(rasio,0)) as pakan, su' +
        'm(nvl(jam,0)) as panjang, sum(nvl(qty,0)) as berat from ipisma_d' +
        'b3.rab_detail'
