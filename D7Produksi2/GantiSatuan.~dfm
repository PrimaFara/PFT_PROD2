  object QItem: TOracleDataSet
    SQL.Strings = (
      
        'select C.ISI,a.no_seri_beam,a.no_reg,a.no_beam,a.jenis_mesin, a.' +
        'rowid, b.konstruksi, b.corak, b.kp/*, a.grup||a.grup2 as grup_ba' +
        'ru*/ '
      
        'from ipisma_db4.beam_mutasi_detail a, ipisma_db4.beam_isi b, bea' +
        'm c'
      
        'where a.lokasi_TUJUAN='#39'KANJI'#39' and a.lokasi_TUJUAN='#39'KANJI'#39' --and ' +
        'a.lokasi='#39'WARPING'#39' and a.lokasi='#39'WARPING'#39
      
        'and b.tanggal>=to_date('#39'01/01/2018'#39','#39'dd/mm/yyyy'#39') and a.no_seri_' +
        'beam=b.no_seri_beam'
      
        'and c.isi=100 and b.no_seri_beam=c.no_seri_beam and a.no_seri_be' +
        'am=c.no_seri_beam'
      'order by b.tanggal'
      '--and a.tanggal<:pakhir ')
